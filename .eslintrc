{
  "parser": "babel-eslint",
  "plugins": [
    "babel",
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "angular": false,
    "d3": false,
    "_": false,
    "describe": false,
    "beforeEach": false,
    "afterEach": false,
    "inject": false,
    "it": false,
    "expect": false,
    "spyOn": false,
    "browser": false,
    "element": false,
    "by": false,
    "protractor": false,
    "debaser": false,
    "sinon": false
  },
  "rules": {
    // Try to follow style guides here:
    // https://github.com/facebook/react-native/blob/master/CONTRIBUTING.md#style-guide
    "react/display-name": [1, {
      "acceptTranspilerName": true
    }],
    "react/jsx-boolean-value": [1, "always"],
    "react/jsx-closing-bracket-location": [1, {
      "nonEmpty": "after-props",
      "selfClosing": "tag-aligned"
    }],
    "react/jsx-curly-spacing": 1,
    "react/jsx-indent-props": [2, 2],
    // don't allow this.fn.bind(this) in JSX callbacks. we don't want to
    // re-create the same fn every render invocation.
    "react/jsx-no-bind": [1, {
      "allowArrowFunctions": true
    }],
    "react/jsx-no-duplicate-props": 1,
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/no-danger": 1,
    "react/no-did-mount-set-state": 1,
    "react/no-did-update-set-state": 1,
    "react/no-multi-comp": [1, {
      "ignoreStateless": true
    }],
    "react/no-unknown-property": 1,
    "react/prop-types": 1,
    "react/react-in-jsx-scope": 1,
    "react/require-extension": 1,
    "react/self-closing-comp": 1,
    "react/sort-comp": 1,
    "react/wrap-multilines": 1,
    "babel/generator-star-spacing": 0,
    "babel/new-cap": 1,
    "babel/object-curly-spacing": 0,
    "babel/object-shorthand": 0,
    "babel/arrow-parens": 0,
    "no-bitwise": 2,
    "camelcase": 0,
    "curly": [
      2,
      "all"
    ],
    "eqeqeq": 2,
    "wrap-iife": 1,
    "indent": [
      0,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-use-before-define": [1, "nofunc"],
    "new-cap": 2,
    "no-caller": 2,
    "quotes": [
      2,
      "single"
    ],
    "no-undef": 2,
    "no-unused-vars": [1, {
      "varsIgnorePattern": "^React$"
    }],
    "strict": 0,
    "dot-notation": [
      0
    ],
    "no-empty": 2,
    "brace-style": [1, "1tbs", {
      "allowSingleLine": true
    }],
    "no-with": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": 1,
    "one-var": [
      1,
      "never"
    ],
    "quote-props": [
      0,
      "as-needed",
      {
        "keywords": true
      }
    ],
    "key-spacing": [
      1,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "space-unary-ops": 0,
    "no-spaced-func": 2,
    "space-before-function-paren": [
      1,
      "never"
    ],
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "comma-dangle": [
      0,
      "never"
    ],
    "no-trailing-spaces": 2,
    "no-debugger": 1,
    "yoda": [
      2,
      "never"
    ],
    "comma-style": [
      2,
      "last"
    ],
    "operator-linebreak": [
      2,
      "after"
    ],
    "space-infix-ops": 1,
    "space-return-throw-case": 0,
    "space-after-keywords": [
      1,
      "always"
    ],
    "lines-around-comment": [
      0
    ],
    "consistent-this": [
      2,
      "self"
    ]
  }
}
