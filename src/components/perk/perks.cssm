.perk {
    max-width: 180px;
    display: flex;
    flex-direction: column;
    margin: 10px auto;
}

.image img {
    max-height: 110px;
    max-width: 145px;
    object-fit: contain;
}
.label {
    text-transform: uppercase;
    color: #1D4E72;
    font-weight: 600;
    font-size: 14px;
    margin-top: 12px;
}
.desc {
    font-size: 12px;
}
