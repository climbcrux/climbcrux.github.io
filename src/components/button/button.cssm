.button {
    margin: 15px 0px;
    padding: 8px 20px;
    border: 3px solid #234473;

    font-size: 12px;
    font-weight: 600;
    line-height: 14px;
    color: #234473;

    text-transform: uppercase;
    text-align: center;

    transition: all 0.2s linear;
}
.button:hover {
    cursor: pointer;
    background: #234473;
    color: white;
}
